{
  "artifactStructure": {
    "assets": [
      {
        "root": "dist",
        "pattern": "assets/**/*"
      }
    ],
    "features": "sites-config/features.json",
    "plugins": [
      {
        "pluginName": "PagesGenerator",
        "sourceFiles": [
          {
            "root": "dist/plugin",
            "pattern": "*{.ts,.json}"
          },
          {
            "root": "dist",
            "pattern": "assets/{server,static,renderer,render}/**/*{.js,.css}"
          }
        ],
        "event": "ON_PAGE_GENERATE",
        "functionName": "PagesGenerator"
      },
      {
        "pluginName": "id-81494",
        "event": "API",
        "functionName": "default",
        "apiPath": "api/entity/{{id}}",
        "sourceFiles": [
          {
            "root": "dist/functions/id-81494",
            "pattern": "*{.js,.ts}"
          }
        ]
      },
      {
        "pluginName": "reviews-86166",
        "event": "API",
        "functionName": "default",
        "apiPath": "api/entity/{{id}}/reviews",
        "sourceFiles": [
          {
            "root": "dist/functions/reviews-86166",
            "pattern": "*{.js,.ts}"
          }
        ]
      },
      {
        "pluginName": "comment-72919",
        "event": "API",
        "functionName": "default",
        "apiPath": "api/entity/{{id}}/review/{{id}}/comment",
        "sourceFiles": [
          {
            "root": "dist/functions/comment-72919",
            "pattern": "*{.js,.ts}"
          }
        ]
      }
    ]
  },
  "dependencies": {
    "installDepsCmd": "npm install",
    "requiredFiles": [
      "package.json",
      "package-lock.json",
      ".npmrc"
    ]
  },
  "buildArtifacts": {
    "buildCmd": "npm run build:local"
  },
  "livePreview": {
    "serveSetupCmd": ":"
  }
}