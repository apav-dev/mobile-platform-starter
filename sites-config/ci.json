{
  "artifactStructure": {
    "assets": [
      {
        "root": "dist",
        "pattern": "assets/**/*"
      }
    ],
    "features": "sites-config/features.json",
    "plugins": [
      {
        "pluginName": "PagesGenerator",
        "sourceFiles": [
          {
            "root": "dist/plugin",
            "pattern": "*{.ts,.json}"
          },
          {
            "root": "dist",
            "pattern": "assets/{server,static,renderer,render}/**/*{.js,.css}"
          }
        ],
        "event": "ON_PAGE_GENERATE",
        "functionName": "PagesGenerator"
      },
      {
        "pluginName": "social-82940",
        "event": "API",
        "functionName": "default",
        "apiPath": "api/social",
        "sourceFiles": [
          {
            "root": "dist/functions/social-82940",
            "pattern": "*{.js,.ts}"
          }
        ]
      },
      {
        "pluginName": "createPost-72629",
        "event": "API",
        "functionName": "default",
        "apiPath": "api/createPost",
        "sourceFiles": [
          {
            "root": "dist/functions/createPost-72629",
            "pattern": "*{.js,.ts}"
          }
        ]
      },
      {
        "pluginName": "entityId-88921",
        "event": "API",
        "functionName": "default",
        "apiPath": "api/entity/{{entityId}}",
        "sourceFiles": [
          {
            "root": "dist/functions/entityId-88921",
            "pattern": "*{.js,.ts}"
          }
        ]
      },
      {
        "pluginName": "reviews-70867",
        "event": "API",
        "functionName": "default",
        "apiPath": "api/entity/{{entityId}}/reviews",
        "sourceFiles": [
          {
            "root": "dist/functions/reviews-70867",
            "pattern": "*{.js,.ts}"
          }
        ]
      },
      {
        "pluginName": "comment-75410",
        "event": "API",
        "functionName": "default",
        "apiPath": "api/entity/{{entityId}}/review/{{reviewId}}/comment",
        "sourceFiles": [
          {
            "root": "dist/functions/comment-75410",
            "pattern": "*{.js,.ts}"
          }
        ]
      }
    ]
  },
  "dependencies": {
    "installDepsCmd": "npm install",
    "requiredFiles": [
      "package.json",
      "package-lock.json",
      ".npmrc"
    ]
  },
  "buildArtifacts": {
    "buildCmd": "npm run build:local"
  },
  "livePreview": {
    "serveSetupCmd": ":"
  }
}